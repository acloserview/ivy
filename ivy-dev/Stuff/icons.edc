#define ICON(_NAME, _FILE, _SIZE) \
   group { \
      name: "e/icons/"_NAME; \
      max: _SIZE _SIZE; \
      parts { \
         part { name: "icon"; mouse_events: 0; \
            description { state: "default" 0.0; \
               max: _SIZE _SIZE; \
               aspect: 1.0 1.0; aspect_preference: BOTH; \
               image.image: "icons/"_FILE COMP; \
               image.normal: "icons/"_FILE; \
            } \
         } \
      } \
   }
   
